{
	"name": "my-app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "npm-run-all --parallel typesafe-i18n sveltekit ",
		"sveltekit": "vite dev --port 3000",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
