{
  "name": "frontend",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm-run-all --parallel sveltekit typesafe-i18n",
    "sveltekit": "vite dev --port 3000",
    "build": "vite build --debug",
    "test": "playwright test",
    "package": "svelte-kit package",
    "preview": "vite preview --port 3000",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "typesafe-i18n": "typesafe-i18n",
    "smui": "svelte-kit sync && npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-node": "^1.1.4",
    "@sveltejs/kit": "^1.1.3",
    "@types/cookie": "^0.5.1",
    "@types/leaflet": "^1.8.0",
    "@types/timelinejs3": "^3.4.3",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "less": "^4.1.3",
    "node-sass": "^7.0.1",
    "nodemon": "^1.3.3",
    "npm-run-all": "^4.1.5",
    "object-hash": "^3.0.0",
    "postcss": "^8.4.12",
    "postcss-load-config": "^3.1.4",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-iso-import": "^0.1.3",
    "vitest": "^0.25.3"
  },
  "type": "module",
  "dependencies": {
    "svelte-i18n": "^3.6.0",
    "typesafe-i18n": "^5.17.1",
    "svelte-leafletjs": "^0.8.2",
    "smui-theme": "^6.0.0-beta.16",
    "@material/theme": "^13.0.0",
    "@smui/icon-button": "^6.0.0-beta.16",
    "@smui/tooltip": "^7.0.0-beta.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@knight-lab/timelinejs": "github:jeromecc/TimelineJS3#d15a6533a43f569c24f03fce5ee39b2d715ba52a",
    "@mdi/js": "^7.0.96",
    "@square/svelte-store": "^1.0.13",
    "@sveltestack/svelte-query": "^1.6.0",
    "cl-editor": "^2.3.0",
    "cookie": "^0.4.2",
    "svelte-material-icons": "^2.0.4",
    "svelte-select": "^4.4.7",
    "sveltestrap": "github:laxadev/sveltestrap"
  }
}
