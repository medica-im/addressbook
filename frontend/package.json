		"@tailwindcss/forms": "^0.5.3",
		"@types/bootstrap": "^5.2.6",
		"@types/cookie": "^0.5.1",
		"@types/leaflet": "^1.9.0",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2",
		"autoprefixer": "^10.4.7",
		"carbon-components-svelte": "^0.73.1",
