{
  "name": "frontend",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm-run-all --parallel sveltekit typesafe-i18n",
    "sveltekit": "vite dev --port 3000",
    "build": "vite build --debug",
    "test": "playwright test",
    "package": "svelte-kit package",
    "preview": "vite preview --port 3000",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "typesafe-i18n": "typesafe-i18n",
    "smui": "svelte-kit sync && npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
    "postinstall": "cd ./node_modules/@knight-lab/timelinejs && npm install && npm run stage_dev"
  },
  "devDependencies": {
    "@material/theme": "^13.0.0",
    "@playwright/test": "^1.28.1",
    "@smui/card": "^6.0.0-beta.16",
    "@smui/circular-progress": "^6.0.0-beta.16",
    "@smui/icon-button": "^6.0.0-beta.16",
    "@smui/list": "^6.0.0-beta.16",
    "@smui/tooltip": "^7.0.0-beta.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-node": "next",
    "@sveltejs/kit": "next",
    "@types/cookie": "^0.5.1",
    "@types/leaflet": "^1.8.0",
    "@types/timelinejs3": "^3.4.3",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "less": "^4.1.3",
    "node-sass": "^7.0.1",
    "nodemon": "^1.3.3",
    "npm-run-all": "^4.1.5",
    "object-hash": "^3.0.0",
    "postcss": "^8.4.12",
    "postcss-load-config": "^3.1.4",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "rollup-plugin-svelte": "^7.1.0",
    "smui-theme": "^6.0.0-beta.16",
    "source-map-support": "^0.5.21",
    "svelte": "^3.54.0",
    "svelte-check": "^2.9.2",
    "svelte-i18n": "^3.6.0",
    "svelte-leafletjs": "^0.8.2",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.4.1",
    "typesafe-i18n": "^5.17.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-iso-import": "^0.1.3",
    "vitest": "0.25.3",
    "yarn-upgrade-all": "^0.7.1"
  },
  "type": "module",
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@knight-lab/timelinejs": "github:jeromecc/TimelineJS3#e70c58c4386af790e224f80345e79a059e1763f1",
    "@mdi/js": "^7.0.96",
    "@square/svelte-store": "^0.2.3",
    "@sveltestack/svelte-query": "^1.6.0",
    "broadcast-channel": "^4.13.0",
    "cl-editor": "^2.3.0",
    "cookie": "^0.4.2",
    "npm": "^9.2.0",
    "pnpm": "^7.0.0",
    "svelte-material-icons": "^2.0.4",
    "svelte-select": "^4.4.7",
    "sveltestrap": "github:laxadev/sveltestrap",
    "vite-plugin-host": "^1.0.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
